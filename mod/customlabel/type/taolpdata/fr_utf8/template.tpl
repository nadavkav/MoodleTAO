<link href="<%%customlabelcss%%>" rel="stylesheet" type="text/css" />
<div class="taolabel">
<table class="taodata">
    <tr>
        <th class="title" colspan="2">
            <%%title%%>
        </th>
    </tr>
    <tr valign="top">
        <td class="param" width="30%">
            Thème du parcours :
        </td>
        <td class="value" width="60%">
            <%%topic%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Objectifs :
        </td>
        <td class="value">
            <%%goal%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Compétences C2i2e visées :
        </td>
        <td class="value">
            <%%c2i%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Discipline(s) :
        </td>
        <td class="value">
            <%%disciplin%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Enseignants concernés :
        </td>
        <td class="value">
            <%%target%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Durée du parcours :
        </td>
        <td class="value">
            <%%duration%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Prérequis de l'enseignant pour s'engager :
        </td>
        <td class="value">
            <%%prerequisite%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Outils, matériel utilisés :
        </td>
        <td class="value">
            <%%equipment%%>
        </td>
    </tr>
</table>
</div>