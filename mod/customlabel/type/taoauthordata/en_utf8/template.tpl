<link href="<%%customlabelcss%%>" rel="stylesheet" type="text/css" />
<div class="taolabel">
<table class="taodata">
    <tr valign="top">
        <th class="title" colspan="2">
            <%%title%%>
        </th>
    </tr>
    <tr valign="top">
        <td class="param" width="30%">
            Author(s):
        </td>
        <td class="value" width="60%">
            <%%authors%%><br/>
            <a href="mailto:<%%contact%%>">Contact the author(s)</a>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Support organization:
        </td>
        <td class="value">
            <%%organization%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Contributor(s) :
        </td>
        <td class="value">
            <%%contributors%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="param">
            Version:
        </td>
        <td class="value">
            <%%version%%>
        </td>
    </tr>
</table>
</div>